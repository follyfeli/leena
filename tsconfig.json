{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/_layout.jsx",
    "test/(tabs)/home.jsx",
    "test/(tabs)/_layout.jsx",
    "test/(tabs)/explore.jsx",
    "app/+not-found.jsx",
    "constants/Colors.js",
    "hooks/useColorScheme.jsx",
    "app/screens/homescreens/homedetails.jsx",
    "nativewind-env.d.ts"
  ]
}